pipeline {
    agent any
    stages {
        stage('Build Project') {
            steps {
                echo 'Building .jar'
            }
        }
        stage('Testing') {
            steps {
                echo 'Running tests'
            }
        }
        stage('Deploying') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Done!') {
            steps {
                echo 'Done :)'
            }
        }
    }
}